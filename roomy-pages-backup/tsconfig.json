{
  "compilerOptions": {
    "lib": [
      "ESNext"
    ],
    // TODO: we shouldn't enable this, but it's the easiest thing to make work right now
    "downlevelIteration": true,
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
}